<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.risingstar.talentaachiva">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TalentaAchiva">
        <activity
            android:name=".DashboardDebugActivity"
            android:exported="false"
            android:label="@string/title_activity_dashboard_debug" />
        <activity
            android:name="com.risingstar.talentaachiva.feature.management.ManagementActivity"
            android:exported="false" />
        <activity
            android:name="com.risingstar.talentaachiva.feature.event.EventDetailActivity"
            android:exported="false" />
        <activity
            android:name="com.risingstar.talentaachiva.feature.landing.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>