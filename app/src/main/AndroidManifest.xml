<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.risingstar.talentaachiva">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TalentaAchiva">
        <activity
            android:name=".feature.organizer.OrganizerActivity"
            android:exported="false"
            android:label="@string/title_activity_organizer"
            android:theme="@style/Theme.TalentaAchiva.NoActionBar" />
        <activity
            android:name=".feature.participant.ParticipantActivity"
            android:exported="false" />
        <activity
            android:name=".feature.judge.JudgeActivity"
            android:exported="false" />
        <activity
            android:name=".feature.detail.DetailActivity"
            android:exported="false"
            android:label="@string/title_activity_detail"
            android:theme="@style/Theme.TalentaAchiva.NoActionBar" />
        <activity
            android:name=".feature.management.ManagementActivity"
            android:exported="false"
            android:label="@string/title_activity_management"
            android:theme="@style/Theme.TalentaAchiva.NoActionBar"/>
        <activity
            android:name=".feature.dashboard.DashboardActivity"
            android:exported="false"
            android:label="@string/title_activity_dashboard"
            android:theme="@style/Theme.TalentaAchiva.NoActionBar" />
        <activity
            android:name=".feature.landing.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>